#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/xyz16/Documents/GitHub/hivefi/fine-tuning/node_modules/.pnpm/@opendocsg+pdf2md@0.2.1/node_modules/@opendocsg/pdf2md/lib/node_modules:/mnt/c/Users/xyz16/Documents/GitHub/hivefi/fine-tuning/node_modules/.pnpm/@opendocsg+pdf2md@0.2.1/node_modules/@opendocsg/pdf2md/node_modules:/mnt/c/Users/xyz16/Documents/GitHub/hivefi/fine-tuning/node_modules/.pnpm/@opendocsg+pdf2md@0.2.1/node_modules/@opendocsg/node_modules:/mnt/c/Users/xyz16/Documents/GitHub/hivefi/fine-tuning/node_modules/.pnpm/@opendocsg+pdf2md@0.2.1/node_modules:/mnt/c/Users/xyz16/Documents/GitHub/hivefi/fine-tuning/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/xyz16/Documents/GitHub/hivefi/fine-tuning/node_modules/.pnpm/@opendocsg+pdf2md@0.2.1/node_modules/@opendocsg/pdf2md/lib/node_modules:/mnt/c/Users/xyz16/Documents/GitHub/hivefi/fine-tuning/node_modules/.pnpm/@opendocsg+pdf2md@0.2.1/node_modules/@opendocsg/pdf2md/node_modules:/mnt/c/Users/xyz16/Documents/GitHub/hivefi/fine-tuning/node_modules/.pnpm/@opendocsg+pdf2md@0.2.1/node_modules/@opendocsg/node_modules:/mnt/c/Users/xyz16/Documents/GitHub/hivefi/fine-tuning/node_modules/.pnpm/@opendocsg+pdf2md@0.2.1/node_modules:/mnt/c/Users/xyz16/Documents/GitHub/hivefi/fine-tuning/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@opendocsg/pdf2md/lib/pdf2md-cli.js" "$@"
else
  exec node  "$basedir/../@opendocsg/pdf2md/lib/pdf2md-cli.js" "$@"
fi
